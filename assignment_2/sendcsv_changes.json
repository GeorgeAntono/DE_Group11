{
  "steps": [
    {
      "name": "gcr.io/cloud-builders/hadoop",
      "entrypoint": "bash",
      "args": [
        "-c",
        "hadoop fs -getmerge gs://data_degroup11/individuals_updated_test/ /workspace/individuals_updated_test.csv"
      ]
    },
    {
      "name": "gcr.io/cloud-builders/hadoop",
      "entrypoint": "bash",
      "args": [
        "-c",
        "hadoop fs -getmerge gs://data_degroup11/spouse_updated_test/ /workspace/spouse_updated_test.csv"
      ]
    },
    {
      "name": "gcr.io/cloud-builders/gsutil",
      "args": ["cp", "/workspace/individuals_updated_test.csv", "gs://data_degroup11/individuals_updated_test.csv"]
    },
    {
      "name": "gcr.io/cloud-builders/gsutil",
      "args": ["cp", "/workspace/spouse_updated_test.csv", "gs://data_degroup11/spouse_updated_test.csv"]
    }
  ],
  "images": [
    "gcr.io/cloud-builders/hadoop:2.10.1",
